modules = ["python-3.11"]

[nix]
channel = "stable-24_05"

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Download spaCy model"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Test Database Connection"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Test and Create User"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Add Calendar FAQs"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Add Sample Advisors"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Train Rasa Model"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Flask App"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Flask Server"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Create Admin"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Add Sample Data"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Database Init"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Sample Data"

[[workflows.workflow]]
name = "Download spaCy model"
author = "agent"

[workflows.workflow.metadata]
agentRequireRestartOnSave = false

[[workflows.workflow.tasks]]
task = "packager.installForAll"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "python -m spacy download en_core_web_sm"

[[workflows.workflow]]
name = "Test Database Connection"
author = "agent"

[workflows.workflow.metadata]
agentRequireRestartOnSave = false

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "python db_test.py"

[[workflows.workflow]]
name = "Test and Create User"
author = "agent"

[workflows.workflow.metadata]
agentRequireRestartOnSave = false

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "python db_test.py"

[[workflows.workflow]]
name = "Add Calendar FAQs"
author = "agent"

[workflows.workflow.metadata]
agentRequireRestartOnSave = false

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "python add_calendar_faqs.py"

[[workflows.workflow]]
name = "Add Sample Advisors"
author = "agent"

[workflows.workflow.metadata]
agentRequireRestartOnSave = false

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "python add_sample_advisors.py"

[[workflows.workflow]]
name = "Train Rasa Model"
author = "agent"

[workflows.workflow.metadata]
agentRequireRestartOnSave = false

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "python train_rasa.py"

[[workflows.workflow]]
name = "Flask App"
author = "agent"

[workflows.workflow.metadata]
agentRequireRestartOnSave = false

[[workflows.workflow.tasks]]
task = "packager.installForAll"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "python main.py"
waitForPort = 5000

[[workflows.workflow]]
name = "Flask Server"
author = "agent"

[workflows.workflow.metadata]
agentRequireRestartOnSave = false

[[workflows.workflow.tasks]]
task = "packager.installForAll"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "python main.py"
waitForPort = 3000

[[workflows.workflow]]
name = "Create Admin"
author = "agent"

[workflows.workflow.metadata]
agentRequireRestartOnSave = false

[[workflows.workflow.tasks]]
task = "packager.installForAll"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "python create_admin.py"

[[workflows.workflow]]
name = "Add Sample Data"
author = "agent"

[workflows.workflow.metadata]
agentRequireRestartOnSave = false

[[workflows.workflow.tasks]]
task = "packager.installForAll"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "python add_sample_advisors.py && python add_calendar_faqs.py"

[[workflows.workflow]]
name = "Database Init"
author = "agent"

[workflows.workflow.metadata]
agentRequireRestartOnSave = false

[[workflows.workflow.tasks]]
task = "packager.installForAll"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "python create_admin.py"

[[workflows.workflow]]
name = "Sample Data"
author = "agent"

[workflows.workflow.metadata]
agentRequireRestartOnSave = false

[[workflows.workflow.tasks]]
task = "packager.installForAll"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "python add_sample_advisors.py && python add_calendar_faqs.py"

[deployment]
run = ["sh", "-c", "python -m spacy download en_core_web_sm"]

[[ports]]
localPort = 3000
externalPort = 3002

[[ports]]
localPort = 5000
externalPort = 80

[[ports]]
localPort = 5005
externalPort = 3000

[[ports]]
localPort = 5055
externalPort = 3001
